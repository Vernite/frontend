{
  "name": "workflow",
  "version": "0.0.1",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build --stats-json",
    "extract:i18n": "ng extract-i18n --output-path=src/locales",
    "//start:docs": "yarn build:docs -s --watch",
    "//build:docs": "compodoc -p tsconfig.doc.json -y src/docs/styles/ --includes src/docs/config/ -a src/docs/assets/",
    "analyze": "webpack-bundle-analyzer dist/workflow/en-US/stats.json",
    "//lint": "ng lint",
    "//start:test": "yarn start:test:firefox",
    "//start:test:firefox": "concurrently \"ng test\" \"http-server --port=9877 ./coverage/workflow\"",
    "//start:test:chrome": "concurrently \"ng test --browsers=Chrome_without_security\" \"http-server --port=9877 ./coverage/workflow\"",
    "//build:test": "ng test --watch=false --code-coverage --browsers=ChromeHeadless",
    "build:badges": "node badge-generator.js",
    "test:e2e": "ng run workflow:e2e",
    "test:e2e:ci": "ng run workflow:e2e-ci",
    "test:unit": "ng run workflow:unit",
    "test:all": "ng run workflow:all",
    "test:all:ci": "ng run workflow:all-ci",
    "test:coverage": "npx nyc report --reporter=lcov --reporter=text-summary",
    "//copy:reports": "(mkdir reports || true) && cp coverage/workflow-e2e/coverage-final.json reports/from-cypress.json && cp coverage/workflow/coverage-final.json reports/from-jest.json",
    "//combine:reports": "npx nyc merge reports && mv coverage.json .nyc_output/out.json",
    "//finalize:combined-report": "npx nyc report --reporter html --reporter text --reporter json-summary --report-dir coverage/workflow-combined"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~13.2.0",
    "@angular/cdk": "^13.3.0",
    "@angular/common": "~13.2.0",
    "@angular/compiler": "~13.2.0",
    "@angular/core": "~13.2.0",
    "@angular/forms": "~13.2.0",
    "@angular/material": "^13.3.0",
    "@angular/platform-browser": "~13.2.0",
    "@angular/platform-browser-dynamic": "~13.2.0",
    "@angular/router": "~13.2.0",
    "@compodoc/compodoc": "^1.1.19",
    "@fortawesome/angular-fontawesome": "^0.10.2",
    "@fortawesome/fontawesome-svg-core": "^6.1.0",
    "@fortawesome/free-brands-svg-icons": "^6.1.0",
    "@fortawesome/free-regular-svg-icons": "^6.1.0",
    "@fortawesome/free-solid-svg-icons": "^6.1.0",
    "@jscutlery/cypress-angular": "^0.9.2",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "chai": "^4.3.6",
    "concurrently": "^7.1.0",
    "dayjs": "^1.11.2",
    "miragejs": "^0.1.43",
    "mocha": "^10.0.0",
    "rxjs": "~7.5.0",
    "tslib": "^2.3.0",
    "ua-parser-js": "^1.0.2",
    "uuid": "^8.3.2",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-builders/custom-webpack": "13.1.0",
    "@angular-devkit/build-angular": "~13.2.5",
    "@angular-eslint/builder": "13.1.0",
    "@angular-eslint/eslint-plugin": "13.1.0",
    "@angular-eslint/eslint-plugin-template": "13.1.0",
    "@angular-eslint/schematics": "13.1.0",
    "@angular-eslint/template-parser": "13.1.0",
    "@angular/cli": "~13.2.5",
    "@angular/compiler-cli": "~13.2.0",
    "@angular/localize": "^13.2.6",
    "@bahmutov/cypress-extends": "^1.1.0",
    "@cypress/code-coverage": "^3.10.0",
    "@cypress/schematic": "^1.6.0",
    "@cypress/webpack-batteries-included-preprocessor": "^2.2.3",
    "@cypress/webpack-dev-server": "^1.8.4",
    "@cypress/webpack-preprocessor": "^5.12.0",
    "@istanbuljs/nyc-config-typescript": "^1.0.2",
    "@jscutlery/cypress-angular-dev-server": "^1.7.13",
    "@jscutlery/cypress-mount": "^0.12.3",
    "@jsdevtools/coverage-istanbul-loader": "^3.0.5",
    "@nrwl/cypress": "^13.1.0",
    "@skyux-sdk/istanbul-instrumenter-loader": "^4.0.0",
    "@types/chai": "^4.3.1",
    "@types/mocha": "^9.1.1",
    "@types/ua-parser-js": "^0.7.36",
    "@types/uuid": "^8.3.4",
    "@typescript-eslint/eslint-plugin": "5.11.0",
    "@typescript-eslint/parser": "5.11.0",
    "autoprefixer": "^10.4.4",
    "badge-maker": "^3.3.1",
    "check-code-coverage": "^1.10.0",
    "core-js": "^3.22.8",
    "cypress": "^10.0.2",
    "eslint": "^8.2.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.0.0",
    "html-webpack-plugin": "^5.5.0",
    "http-server": "^14.1.0",
    "istanbul-lib-coverage": "^3.2.0",
    "jasmine-core": "~4.0.0",
    "jsdom": "^19.0.0",
    "lcov-result-merger": "^3.2.0",
    "ngx-build-plus": "^13.0.1",
    "nyc": "^15.1.0",
    "postcss": "^8.4.12",
    "prettier": "^2.6.0",
    "stylelint": "^14.6.0",
    "stylelint-config-idiomatic-order": "^8.1.0",
    "stylelint-config-sass-guidelines": "^9.0.1",
    "stylelint-config-standard": "^25.0.0",
    "stylelint-config-standard-scss": "^3.0.0",
    "tailwindcss": "^3.0.23",
    "typescript": "~4.5.2",
    "webpack": "^5.72.1",
    "webpack-bundle-analyzer": "^4.5.0",
    "webpack-dev-server": "^4.9.1"
  }
}
